{
    "name": "thomasvargiu/tmv-whatsapi",
    "description": "Library to communicate with WhatsApp service",
    "type": "library",
    "keywords": [
        "whatsapp",
        "whatsapi",
        "sms"
    ],
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Thomas Vargiu",
            "email": "tommy@ttech.it",
            "homepage": "http://thomasvargiu.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tmv\\WhatsApi\\": ["src/", "tests/unit/"]
        }
    },
    "require": {
        "php": ">=5.3.6",
        "ext-curl": "*",
        "zendframework/zend-eventmanager": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "@stable",
        "phpmd/phpmd" : "1.4.*",
        "squizlabs/php_codesniffer": "1.*",
        "fabpot/php-cs-fixer": "0.*@dev",
        "satooshi/php-coveralls": "dev-master"
    }
}